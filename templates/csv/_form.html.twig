
    <div class="modal fade" id="modalImportForm" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog bg-transparent" role="document">
            <div class="modal-content">
                <div class="modal-header text-center">
                    <h4 class="modal-title w-100 font-weight-bold card blue-gradient text-white py-3">Téléverser le rapport</h4>
                    <button type="button" class="close rgba-black-strong font-weight-bold" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" class="h4 font-weight-bold">&times;</span>
                    </button>
                </div>                
                <form enctype="multipart/form-data" id="rapportForm">
                    <div class="modal-body mx-3"> 
                        <div class="justify-content-center">
                            <span id="message"></span>
                        </div>
                        <div class="md-form mb-5">
                            
                            {# <div class="md-form col-lg-8 col-md-12">   #}
                                <div class="file-field">
                                    
                                    <div class="btn btn-primary btn-sm float-left">
                                        <i class='fas fa-file-excel prefix'></i><span>o</span>
                                        <input type="file" name="file" class="fileImport" accept=".xls, .xlxs, .csv">
                                    </div>
                                    <div class="file-path-wrapper">
                                        <input class="file-path validate fileImport" type="text" placeholder="En attente de fichier..." >
                                    </div>     
                            {# </div> #}
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer d-flex justify-content-center">
                        <button class="btn btn-outline-primary btn-rounded" name="import" id="import">Importer</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

